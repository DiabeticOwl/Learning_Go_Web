{{define "people"}}
    <table>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Age</th>
        </tr>
        {{range .}}
        <tr>
            <td>{{.Name}}</td>
            <td>{{.Surname}}</td>
            <td>{{.Age}}</td>
        </tr>
        {{end}}
    </table>
{{end}}